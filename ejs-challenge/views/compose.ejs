<%- include('./partials/header.ejs')   -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="postTitles">Title</label>
        <input type="text" name="postTitles" id="postTitles" class="form-control">

        <label for="postBody">Post</label>
        <textarea class="form-control" name="postBody" id="postBody" cols="80" rows="5"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>

</form>


